[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../rust-tetris/target/debug/librust_tetris.so"
linux.release.x86_64 =   "res://../rust-tetris/target/release/librust_tetris.so"
windows.debug.x86_64 =   "res://../rust-tetris/target/debug/rust_tetris.dll"
windows.release.x86_64 = "res://../rust-tetris/target/release/rust_tetris.dll"
macos.debug =            "res://../rust-tetris/target/debug/librust_tetris.dylib"
macos.release =          "res://../rust-tetris/target/release/librust_tetris.dylib"
macos.debug.arm64 =      "res://../rust-tetris/target/debug/librust_tetris.dylib"
macos.release.arm64 =    "res://../rust-tetris/target/release/librust_tetris.dylib"
